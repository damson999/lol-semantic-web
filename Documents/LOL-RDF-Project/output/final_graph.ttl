@prefix : <http://example.org/lol#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

# Archivo principal que importa ontología y datos
<http://example.org/lol>
    a owl:Ontology ;
    owl:imports <http://example.org/lol/ontology> ;
    owl:imports <http://example.org/lol/data> .

# Link simbólico a tu ontología real
<http://example.org/lol/ontology>
    owl:imports <file:ontology/lol.owl> .

# Link simbólico a tus datos reales
<http://example.org/lol/data>
    owl:imports <file:output/champions.ttl> .
