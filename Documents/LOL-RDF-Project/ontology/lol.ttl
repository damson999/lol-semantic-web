@prefix : <http://example.org/lol#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:Champion a rdfs:Class .
:Role a rdfs:Class .
:Stat a rdfs:Class .

:hasRole a rdf:Property ;
    rdfs:domain :Champion ;
    rdfs:range :Role .

:hasStat a rdf:Property ;
    rdfs:domain :Champion ;
    rdfs:range :Stat .

:statName a rdf:Property ;
    rdfs:domain :Stat ;
    rdfs:range xsd:string .

:statValue a rdf:Property ;
    rdfs:domain :Stat ;
    rdfs:range xsd:float .

:isMelee a owl:DatatypeProperty ;
    rdfs:domain :Champion ;
    rdfs:range xsd:boolean .

:isRanged a owl:DatatypeProperty ;
    rdfs:domain :Champion ;
    rdfs:range xsd:boolean .

:HighDamageChampion a owl:Class ;
    owl:equivalentClass [
        a owl:Class ;
        owl:intersectionOf (
            :Champion
            [ a owl:Restriction ;
              owl:onProperty :hasStat ;
              owl:someValuesFrom [
                    a owl:Class ;
                    owl:intersectionOf (
                        :Stat
                        [ a owl:Restriction ;
                          owl:onProperty :statName ;
                          owl:hasValue "attackdamage" ]
                        [ a owl:Restriction ;
                          owl:onProperty :statValue ;
                          owl:someValuesFrom xsd:float ;
                          owl:withRestrictions ( [ xsd:minInclusive 60 ] )
                        ]
                    )
              ]
            ]
        )
    ] .

